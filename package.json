{
  "name": "@apteco/ngth",
  "version": "1.4.4",
  "description": "ng-bullet successor to help speed up angular testing",
  "keywords": [
    "ng",
    "angular",
    "unit testing",
    "testing",
    "performance",
    "typescript",
    "jasmine",
    "TestBed"
  ],
  "license": "MIT",
  "author": {
    "name": "Matt Collinge",
    "email": "matthew.collinge@apteco.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Apteco/ngth.git"
  },
  "homepage": "https://github.com/Apteco/ngth",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:watch": "ng test",
    "test:watch-headless": "ng test --browsers=ChromeHeadless",
    "lint": "ng lint",
    "publish": "npm run build:prod && cd ./dist/ngth && npm publish --access public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "14.0.7",
    "@angular/common": "14.0.7",
    "@angular/compiler": "14.0.7",
    "@angular/core": "14.0.7",
    "@angular/forms": "14.0.7",
    "@angular/platform-browser": "14.0.7",
    "@angular/platform-browser-dynamic": "14.0.7",
    "@angular/router": "14.0.7",
    "rxjs": "7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-devkit/core": "13.2.2",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "14.0.7",
    "@angular/language-service": "13.2.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "4.1.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "14.1.0",
    "np": "5.0.3",
    "protractor": "7.0.0",
    "ts-node": "7.0.0",
    "tslint": "6.1.3",
    "typescript": "4.7.4"
  }
}
